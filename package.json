{
  "name": "newproject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:run:dev": "react-native run-android --variant=devDebug --appIdSuffix=dev",
    "android:run:qa": "react-native run-android --variant=qaDebug --appIdSuffix=qa",
    "android:run:prod": "react-native run-android --variant=prodDebug",
    "android:build:dev": "cd android && ./gradlew assembleDevRelease",
    "android:build:qa": "cd android && ./gradlew assembleQaRelease",
    "android:build:prod": "cd android && ./gradlew assembleProdRelease",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "expo-localization": "^14.0.0",
    "i18n-js": "^4.2.2",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-async-storage": "^0.0.1",
    "react-native-code-push": "^7.1.0",
    "react-native-config": "^1.5.0",
    "react-native-elements": "^3.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
